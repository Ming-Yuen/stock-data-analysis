            <column name="DEC_PROP_1" type="NUMBER(13,4)"/>
            <column name="DEC_PROP_2" type="NUMBER(13,4)"/>
            <column name="BOOL_PROP_1" type="VARCHAR2(1)"/>
            <column name="BOOL_PROP_2" type="VARCHAR2(1)"/>
        </createTable>

        <addPrimaryKey tableName="QRTZ_SIMPROP_TRIGGERS"

        <insert tableName="job_config">
            <column name="task_name" value="StockDailyTask"/>
            <column name="task_group" value="SCHEDULE"/>
            <column name="task_description" value="Daily task to update stock symbols for all exchanges"/>
            <column name="job_class_name" value="com.stockinsight.scheduling.StockDailyTask"/>
            <column name="cron_expression" value="0 0 6 * * ?"/>
            <column name="trigger_type" value="CRON"/>
            <column name="config_Json" value="{}"/>
            <column name="enabled" valueBoolean="true"/>
            <column name="start_time" valueDate="${now}"/>
            <column name="retry_count" valueNumeric="0"/>
            <column name="max_retry_count" valueNumeric="3"/>
            <column name="status" value="WAITING"/>
            <column name="created_by" value="${admin}"/>
            <column name="created_at" valueDate="${now}"/>
            <column name="updated_by" value="${admin}"/>
            <column name="updated_at" valueDate="${now}"/>
            <column name="is_deleted" valueNumeric="0"/>
            <column name="remark" value="Auto-generated task for daily stock data updates"/>
            <column name="version" valueNumeric="1"/>
